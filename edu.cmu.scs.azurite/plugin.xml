<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <category
            name="Azurite"
            id="edu.cmu.scs.azurite">
      </category>
      <view
            category="edu.cmu.scs.azurite"
            class="edu.cmu.scs.azurite.views.GraphViewPart"
            icon="icons/azurite.gif"
            id="edu.cmu.scs.azurite.views.GraphViewPart"
            name="Graph View">
      </view>
      <view
            category="edu.cmu.scs.azurite"
            class="edu.cmu.scs.azurite.views.TimelineViewPart"
            icon="icons/timeline_marker.png"
            id="edu.cmu.scs.azurite.views.TimelineViewPart"
            name="Timeline View"
            restorable="true">
      </view>
      <view
            allowMultiple="true"
            category="edu.cmu.scs.azurite"
            class="edu.cmu.scs.azurite.views.CodeHistoryDiffViewPart"
            icon="icons/time_machine.png"
            id="edu.cmu.scs.azurite.views.CodeHistoryDiffViewPart"
            name="Code History Diff"
            restorable="false">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="edu.cmu.scs.azurite.plugin.AzuriteStartup">
      </startup>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:#CompilationUnitEditorContext?after=additions">
         <menu
               label="Azurite">
            <command
                  commandId="edu.cmu.scs.azurite.ui.commands.selectCorrespondingTimelineRectanglesCommand"
                  style="push">
            </command>
            <command
                  commandId="edu.cmu.scs.azurite.ui.commands.undoInRegionCommand"
                  style="push">
            </command>
            <command
                  commandId="edu.cmu.scs.azurite.ui.commands.launchCodeHistoryDiff"
                  style="push">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.ui.main.toolbar">
         <toolbar
               id="edu.cmu.scs.azurite.toolbar1"
               label="Azurite Toolbar">
         </toolbar>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               id="Azurite Menu"
               label="Azurite"
               mnemonic="z">
            <menu
                  label="Selected Code in the Editor">
               <command
                     commandId="edu.cmu.scs.azurite.ui.commands.selectCorrespondingTimelineRectanglesCommand"
                     style="push">
               </command>
               <command
                     commandId="edu.cmu.scs.azurite.ui.commands.undoInRegionCommand"
                     style="push">
               </command>
               <command
                     commandId="edu.cmu.scs.azurite.ui.commands.launchCodeHistoryDiff"
                     style="push">
               </command>
            </menu>
            <menu
                  label="Selected Rectangles in Timeline">
               <command
                     commandId="edu.cmu.scs.azurite.ui.commands.selectiveUndoCommand"
                     style="push">
               </command>
               <command
                     commandId="edu.cmu.scs.azurite.ui.commands.highlightCodeCommand"
                     style="push">
               </command>
               <command
                     commandId="edu.cmu.scs.azurite.ui.commands.undoEverythingAfterSelectionCommand"
                     style="push">
               </command>
               <command
                     commandId="edu.cmu.scs.azurite.ui.commands.jumpToTheAffectedCodeCommand"
                     style="push">
               </command>
            </menu>
            <menu
                  label="Selected Time Range in the Timeline">
               <command
                     commandId="edu.cmu.scs.azurite.ui.commands.showFilesInRangeCommand"
                     style="push">
               </command>
               <command
                     commandId="edu.cmu.scs.azurite.ui.commands.openFilesInRangeCommand"
                     style="push">
               </command>
            </menu>
            <separator
                  name="edu.cmu.scs.azurite.separator1"
                  visible="true">
            </separator>
            <menu
                  label="Timeline View">
               <command
                     commandId="edu.cmu.scs.azurite.ui.commands.compactModeCommand"
                     style="toggle">
               </command>
               <separator
                     name="edu.cmu.scs.azurite.separator1"
                     visible="true">
               </separator>
               <command
                     commandId="edu.cmu.scs.azurite.ui.commands.executeJSCode"
                     icon="icons/old_go_first.png"
                     label="Scroll to Start"
                     style="push"
                     tooltip="Scroll to Start">
                  <parameter
                        name="edu.cmu.scs.azurite.ui.commands.executeJSCode.codeToExecute"
                        value="translateX(0);">
                  </parameter>
               </command>
               <command
                     commandId="edu.cmu.scs.azurite.ui.commands.executeJSCode"
                     icon="icons/old_go_previous.png"
                     label="Scroll Left"
                     style="push"
                     tooltip="Scroll Left">
                  <parameter
                        name="edu.cmu.scs.azurite.ui.commands.executeJSCode.codeToExecute"
                        value="showBefore();">
                  </parameter>
               </command>
               <command
                     commandId="edu.cmu.scs.azurite.ui.commands.executeJSCode"
                     icon="icons/old_go_next.png"
                     label="Scroll Right"
                     style="push"
                     tooltip="Scroll Right">
                  <parameter
                        name="edu.cmu.scs.azurite.ui.commands.executeJSCode.codeToExecute"
                        value="showAfter();">
                  </parameter>
               </command>
               <command
                     commandId="edu.cmu.scs.azurite.ui.commands.executeJSCode"
                     icon="icons/old_go_last.png"
                     label="Scroll to End"
                     style="push"
                     tooltip="Scroll to End">
                  <parameter
                        name="edu.cmu.scs.azurite.ui.commands.executeJSCode.codeToExecute"
                        value="scrollToEnd();">
                  </parameter>
               </command>
               <separator
                     name="edu.cmu.scs.azurite.separator2"
                     visible="true">
               </separator>
               <command
                     commandId="edu.cmu.scs.azurite.ui.commands.executeJSCode"
                     icon="icons/old_go_up.png"
                     label="Scroll Up"
                     style="push"
                     tooltip="Scroll Up">
                  <parameter
                        name="edu.cmu.scs.azurite.ui.commands.executeJSCode.codeToExecute"
                        value="showUp();">
                  </parameter>
               </command>
               <command
                     commandId="edu.cmu.scs.azurite.ui.commands.executeJSCode"
                     icon="icons/old_go_down.png"
                     label="Scroll Down"
                     style="push"
                     tooltip="Scroll Down">
                  <parameter
                        name="edu.cmu.scs.azurite.ui.commands.executeJSCode.codeToExecute"
                        value="showDown();">
                  </parameter>
               </command>
               <separator
                     name="edu.cmu.scs.azurite.separator3"
                     visible="true">
               </separator>
               <command
                     commandId="edu.cmu.scs.azurite.ui.commands.executeJSCode"
                     label="Zoom In Horizontally"
                     style="push"
                     tooltip="Zoom In Horizontally">
                  <parameter
                        name="edu.cmu.scs.azurite.ui.commands.executeJSCode.codeToExecute"
                        value="barZoomIn();">
                  </parameter>
               </command>
               <command
                     commandId="edu.cmu.scs.azurite.ui.commands.executeJSCode"
                     label="Zoom Out Horizontally"
                     style="push"
                     tooltip="Zoom Out Horizontally">
                  <parameter
                        name="edu.cmu.scs.azurite.ui.commands.executeJSCode.codeToExecute"
                        value="barZoomOut();">
                  </parameter>
               </command>
               <command
                     commandId="edu.cmu.scs.azurite.ui.commands.executeJSCode"
                     label="Zoom In Vertically"
                     style="push"
                     tooltip="Zoom In Vertically">
                  <parameter
                        name="edu.cmu.scs.azurite.ui.commands.executeJSCode.codeToExecute"
                        value="fileZoomIn();">
                  </parameter>
               </command>
               <command
                     commandId="edu.cmu.scs.azurite.ui.commands.executeJSCode"
                     label="Zoom Out Vertically"
                     style="push"
                     tooltip="Zoom Out Vertically">
                  <parameter
                        name="edu.cmu.scs.azurite.ui.commands.executeJSCode.codeToExecute"
                        value="fileZoomOut();">
                  </parameter>
               </command>
               <separator
                     name="edu.cmu.scs.azurite.separator4"
                     visible="true">
               </separator>
               <command
                     commandId="edu.cmu.scs.azurite.ui.commands.refreshTimeline"
                     style="push">
               </command>
            </menu>
            <separator
                  name="edu.cmu.scs.azurite.separator2"
                  visible="true">
            </separator>
            <command
                  commandId="edu.cmu.scs.azurite.ui.commands.historySearchCommand"
                  style="push">
            </command>
            <separator
                  name="edu.cmu.scs.azurite.separator3"
                  visible="true">
            </separator>
            <command
                  commandId="edu.cmu.scs.azurite.ui.commands.readPreviousLog"
                  style="push">
            </command>
            <separator
                  name="edu.cmu.scs.azurite.separator4"
                  visible="true">
            </separator>
            <command
                  commandId="edu.cmu.scs.fluorite.annotateCommand"
                  style="push"
                  tooltip="Please help the researcher by describing the intent of your recent backtracking / code editing">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:edu.cmu.scs.azurite.views.TimelineViewPart">
         <command
               commandId="edu.cmu.scs.azurite.ui.commands.readPreviousLog"
               style="push">
         </command>
         <command
               commandId="edu.cmu.scs.azurite.ui.commands.launchCodeHistoryDiff"
               style="push">
         </command>
         <separator
               name="edu.cmu.scs.azurite.separator1"
               visible="true">
         </separator>
         <command
               commandId="edu.cmu.scs.azurite.ui.commands.compactModeCommand"
               style="toggle">
         </command>
         <separator
               name="edu.cmu.scs.azurite.separator2"
               visible="true">
         </separator>
         <command
               commandId="edu.cmu.scs.azurite.ui.commands.executeJSCode"
               icon="icons/old_go_first.png"
               label="Scroll to Start"
               style="push"
               tooltip="Scroll to Start">
            <parameter
                  name="edu.cmu.scs.azurite.ui.commands.executeJSCode.codeToExecute"
                  value="translateX(0);">
            </parameter>
         </command>
         <command
               commandId="edu.cmu.scs.azurite.ui.commands.executeJSCode"
               icon="icons/old_go_previous.png"
               label="Scroll Left"
               style="push"
               tooltip="Scroll Left">
            <parameter
                  name="edu.cmu.scs.azurite.ui.commands.executeJSCode.codeToExecute"
                  value="showBefore();">
            </parameter>
         </command>
         <command
               commandId="edu.cmu.scs.azurite.ui.commands.executeJSCode"
               icon="icons/old_go_next.png"
               label="Scroll Right"
               style="push"
               tooltip="Scroll Right">
            <parameter
                  name="edu.cmu.scs.azurite.ui.commands.executeJSCode.codeToExecute"
                  value="showAfter();">
            </parameter>
         </command>
         <command
               commandId="edu.cmu.scs.azurite.ui.commands.executeJSCode"
               icon="icons/old_go_last.png"
               label="Scroll to End"
               style="push"
               tooltip="Scroll to End">
            <parameter
                  name="edu.cmu.scs.azurite.ui.commands.executeJSCode.codeToExecute"
                  value="scrollToEnd();">
            </parameter>
         </command>
         <separator
               name="edu.cmu.scs.azurite.separator3"
               visible="true">
         </separator>
         <command
               commandId="edu.cmu.scs.azurite.ui.commands.executeJSCode"
               icon="icons/old_go_up.png"
               label="Scroll Up"
               style="push"
               tooltip="Scroll Up">
            <parameter
                  name="edu.cmu.scs.azurite.ui.commands.executeJSCode.codeToExecute"
                  value="showUp();">
            </parameter>
         </command>
         <command
               commandId="edu.cmu.scs.azurite.ui.commands.executeJSCode"
               icon="icons/old_go_down.png"
               label="Scroll Down"
               style="push"
               tooltip="Scroll Down">
            <parameter
                  name="edu.cmu.scs.azurite.ui.commands.executeJSCode.codeToExecute"
                  value="showDown();">
            </parameter>
         </command>
         <separator
               name="edu.cmu.scs.azurite.separator4"
               visible="true">
         </separator>
         <command
               commandId="edu.cmu.scs.azurite.ui.commands.refreshTimeline"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="edu.cmu.scs.azurite.ui.handlers.SelectOperationsInRegion"
            id="edu.cmu.scs.azurite.ui.commands.selectCorrespondingTimelineRectanglesCommand"
            name="Select Corresponding Timeline Rectangles">
      </command>
      <command
            defaultHandler="edu.cmu.scs.azurite.ui.handlers.UndoInRegionHandler"
            id="edu.cmu.scs.azurite.ui.commands.undoInRegionCommand"
            name="Undo All Operations on the Selection">
      </command>
      <command
            defaultHandler="edu.cmu.scs.azurite.ui.handlers.LaunchCodeHistoryDiffHandler"
            id="edu.cmu.scs.azurite.ui.commands.launchCodeHistoryDiff"
            name="Launch Code History Diff">
      </command>
      <command
            defaultHandler="edu.cmu.scs.azurite.ui.handlers.SelectiveUndoHandler"
            id="edu.cmu.scs.azurite.ui.commands.selectiveUndoCommand"
            name="Selective Undo">
      </command>
      <command
            id="edu.cmu.scs.azurite.ui.commands.highlightCodeCommand"
            name="Highlight Code Affected by the Selected Rectangles">
      </command>
      <command
            defaultHandler="edu.cmu.scs.azurite.ui.handlers.UndoEverythingAfterSelectionHandler"
            id="edu.cmu.scs.azurite.ui.commands.undoEverythingAfterSelectionCommand"
            name="Undo Everything After the Last Selected Rectangle">
      </command>
      <command
            defaultHandler="edu.cmu.scs.azurite.ui.handlers.JumpToAffectedCodeHandler"
            id="edu.cmu.scs.azurite.ui.commands.jumpToTheAffectedCodeCommand"
            name="Jump to the Affected Code in the Editor">
      </command>
      <command
            id="edu.cmu.scs.azurite.ui.commands.showFilesInRangeCommand"
            name="Show Files Edited in Selected Time Range in Timeline">
      </command>
      <command
            id="edu.cmu.scs.azurite.ui.commands.openFilesInRangeCommand"
            name="Open Files Edited in Selected Time Range in the Editor">
      </command>
      <command
            defaultHandler="edu.cmu.scs.azurite.ui.handlers.ActivateFirebugLiteHandler"
            id="edu.cmu.scs.azurite.ui.commands.activateFirebugLiteCommand"
            name="Activate Firebug Lite">
      </command>
      <command
            defaultHandler="edu.cmu.scs.azurite.ui.handlers.ReadPreviousLogHandler"
            id="edu.cmu.scs.azurite.ui.commands.readPreviousLog"
            name="Read Previous Log">
      </command>
      <command
            defaultHandler="edu.cmu.scs.azurite.ui.handlers.ExecuteJSCodeHandler"
            id="edu.cmu.scs.azurite.ui.commands.executeJSCode"
            name="Execute Javascript Code">
         <commandParameter
               id="edu.cmu.scs.azurite.ui.commands.executeJSCode.codeToExecute"
               name="Code to Execute"
               optional="false">
         </commandParameter>
      </command>
      <command
            defaultHandler="edu.cmu.scs.azurite.ui.handlers.RefreshTimelineHandler"
            id="edu.cmu.scs.azurite.ui.commands.refreshTimeline"
            name="Refresh Timeline">
      </command>
      <command
            defaultHandler="edu.cmu.scs.azurite.ui.handlers.HistorySearchHandler"
            id="edu.cmu.scs.azurite.ui.commands.historySearchCommand"
            name="History Search">
      </command>
      <command
            defaultHandler="edu.cmu.scs.azurite.ui.handlers.CompactModeHandler"
            id="edu.cmu.scs.azurite.ui.commands.compactModeCommand"
            name="Compact Mode">
         <state
               class="org.eclipse.ui.handlers.RegistryToggleState:true"
               id="org.eclipse.ui.commands.toggleState">
         </state>
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="edu.cmu.scs.azurite.ui.commands.activateFirebugLiteCommand"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M2+M3+F">
      </key>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="edu.cmu.scs.azurite.preferences.Initializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.commandImages">
      <image
            commandId="edu.cmu.scs.azurite.ui.commands.undoInRegionCommand"
            icon="icons/undo_in_region.png">
      </image>
      <image
            commandId="edu.cmu.scs.azurite.ui.commands.launchCodeHistoryDiff"
            icon="icons/time_machine.png">
      </image>
      <image
            commandId="edu.cmu.scs.azurite.ui.commands.readPreviousLog"
            icon="icons/previous.png">
      </image>
      <image
            commandId="edu.cmu.scs.azurite.ui.commands.compactModeCommand"
            icon="icons/compress.png">
      </image>
      <image
            commandId="edu.cmu.scs.azurite.ui.commands.refreshTimeline"
            icon="icons/refresh_red.png">
      </image>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="*">
         <view
               closeable="true"
               id="edu.cmu.scs.azurite.views.TimelineViewPart"
               minimized="false"
               moveable="true"
               ratio="0.5"
               relationship="top"
               relative="edu.cmu.scs.azurite.views.TimelineViewPart"
               showTitle="true"
               standalone="false"
               visible="false">
         </view>
      </perspectiveExtension>
   </extension>

</plugin>
